}

.unlock_num1{
    width: 128.5rpx;
    display: inline-block;
    text-align: center;
}

.finish_button {
    position: absolute;
    left: 76rpx;
    right: 0;
    bottom: 114rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 32rpx;
    color: #858989;
    width: 600rpx;
    height: 90rpx;
}

.finish {
    border-radius: 10rpx;
    color: #1992FC;
    background-color: #FFFFFF;
    border:2rpx solid #1992FC;
}
